<site-list version="10">
  <!-- Development -->
  <site url="localhost:5001">
    <compat-mode>Default</compat-mode>
    <open-in>MSEdge</open-in>
  </site>
  <site url="localhost:5001/Edge">
    <compat-mode>Default</compat-mode>
    <open-in>MSEdge</open-in>
  </site>
  <site url="localhost:5001/IE">
    <compat-mode>Default</compat-mode>
    <open-in>IE11</open-in>
  </site>
  <site url="localhost:5001/Account">
    <compat-mode>Default</compat-mode>
    <open-in>None</open-in>
  </site>

  <!-- MSEdge, IE11, Both -->
  <!--<shared-cookie domain="localhost" name="custom-cookie-name" source-engine="Both"></shared-cookie>-->
  <shared-cookie host="localhost" name="custom-cookie-name" source-engine="Both"></shared-cookie>
  <shared-cookie host="localhost" name="custom-antiforgery-cookie-name" source-engine="Both"></shared-cookie>

  <!-- CookieSso -->
  <site url="app1.company.local">
    <compat-mode>Default</compat-mode>
    <open-in>MSEdge</open-in>
  </site>
  <site url="app2.company.local">
    <compat-mode>Default</compat-mode>
    <open-in>IE11</open-in>
  </site>
  <site url="identity.company.local">
    <compat-mode>Default</compat-mode>
    <open-in>IE11</open-in>
  </site>
  <shared-cookie domain=".company.local" name="CookieSso.Identity" source-engine="Both"></shared-cookie>

  <!-- Production -->
  <site url="ie-mode-sample-webapp.azurewebsites.net">
    <compat-mode>Default</compat-mode>
    <open-in>MSEdge</open-in>
  </site>
  <site url="ie-mode-sample-webapp.azurewebsites.net/Edge">
    <compat-mode>Default</compat-mode>
    <open-in>MSEdge</open-in>
  </site>
  <site url="ie-mode-sample-webapp.azurewebsites.net/IE">
    <compat-mode>Default</compat-mode>
    <open-in>IE11</open-in>
  </site>
  <site url="ie-mode-sample-webapp.azurewebsites.net/Account">
    <compat-mode>Default</compat-mode>
    <open-in>None</open-in>
  </site>

  <!-- MSEdge, IE11, Both -->
  <!--<shared-cookie domain="ie-mode-sample-webapp.azurewebsites.net" name="custom-cookie-name" source-engine="Both"></shared-cookie>-->
  <shared-cookie host="ie-mode-sample-webapp.azurewebsites.net" name="custom-cookie-name" source-engine="Both"></shared-cookie>
  <shared-cookie host="ie-mode-sample-webapp.azurewebsites.net" name="custom-antiforgery-cookie-name" source-engine="Both"></shared-cookie>
</site-list>
